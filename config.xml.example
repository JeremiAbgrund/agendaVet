<?xml version='1.0' encoding='utf-8'?>
<widget id="cl.duoc.tuapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>AgendaVet</name>
  <description>Aplicación híbrida para agendamiento y gestión de citas veterinarias.</description>
  <author email="dev@agendavet.cl" href="https://agendavet.example">AgendaVet Team</author>

  <!-- Android-specific preferences (se pueden aplicar vía gradle o configuración de plataforma) -->
  <preference name="android-minSdkVersion" value="21" />
  <preference name="android-targetSdkVersion" value="33" />

  <!-- Plataformas previstas -->
  <platform name="android" />
  <platform name="ios" />
  <platform name="browser" />
</widget>